<list>
  <com.axibase.collector.model.job.snmp.SnmpJobConfig>
    <name>snmp-щы</name>
    <cronExpression>R 0/5 * * * ?</cronExpression>
    <enabled>true</enabled>
    <jobType>SNMP</jobType>
    <defaultTags/>
    <checkLastTime>false</checkLastTime>
    <commitInterval>10</commitInterval>
    <errorThreshold>0</errorThreshold>
    <configurations>
      <com.axibase.collector.model.job.snmp.SnmpConfiguration>
        <configName>system statistics</configName>
        <transport>UDP</transport>
        <port>161</port>
        <mibName>UCD-SNMP-MIB</mibName>
        <timeout>3</timeout>
        <maxRepetitions>10</maxRepetitions>
        <nonRepeaters>0</nonRepeaters>
        <version>V2C</version>
        <community>public</community>
        <metricPrefix>snmp.</metricPrefix>
        <entitySet>
          <name>SNMP Devices</name>
          <type>TEXT</type>
          <discardDuplicates>false</discardDuplicates>
          <entities>
            <string>192.0.2.1</string>
            <string>192.0.2.2</string>
          </entities>
        </entitySet>
        <tagNames>
          <string>dskPath</string>
          <string>laNames</string>
          <string>dskDevice</string>
        </tagNames>
        <metricColumns>
          <string>dskUsed</string>
          <string>dskTotal</string>
          <string>ssCpuIdle</string>
          <string>dskPercent</string>
          <string>memTotalReal</string>
          <string>ssSwapIn</string>
          <string>memCached</string>
          <string>ssCpuUser</string>
          <string>memTotalSwap</string>
          <string>memShared</string>
          <string>memBuffer</string>
          <string>ssCpuSystem</string>
          <string>memAvailReal</string>
          <string>memAvailSwap</string>
          <string>laLoad</string>
          <string>memTotalFree</string>
          <string>ssSwapOut</string>
        </metricColumns>
        <retries>3</retries>
        <securityName>username</securityName>
        <authenticationPassPhrase>password</authenticationPassPhrase>
        <authProtocol>MD5</authProtocol>
        <privacyProtocol>DES</privacyProtocol>
        <secLevel>AUTH_PRIV</secLevel>
      </com.axibase.collector.model.job.snmp.SnmpConfiguration>
      <com.axibase.collector.model.job.snmp.SnmpConfiguration>
        <configName>interface statistics</configName>
        <transport>UDP</transport>
        <port>161</port>
        <mibName>IF-MIB</mibName>
        <timeout>3</timeout>
        <maxRepetitions>10</maxRepetitions>
        <nonRepeaters>0</nonRepeaters>
        <version>V2C</version>
        <community>public</community>
        <metricPrefix>snmp.</metricPrefix>
        <entitySet reference="../../com.axibase.collector.model.job.snmp.SnmpConfiguration/entitySet"/>
        <tagNames>
          <string>ifDescr</string>
        </tagNames>
        <metricColumns>
          <string>ifOutOctets</string>
          <string>ifInDiscards</string>
          <string>ifOutErrors</string>
          <string>ifInOctets</string>
          <string>ifInErrors</string>
          <string>ifOutDiscards</string>
        </metricColumns>
        <retries>3</retries>
        <securityName>username</securityName>
        <authenticationPassPhrase>password</authenticationPassPhrase>
        <authProtocol>MD5</authProtocol>
        <privacyProtocol>DES</privacyProtocol>
        <secLevel>AUTH_PRIV</secLevel>
      </com.axibase.collector.model.job.snmp.SnmpConfiguration>
    </configurations>
  </com.axibase.collector.model.job.snmp.SnmpJobConfig>
</list>